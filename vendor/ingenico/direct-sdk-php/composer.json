{
    "name": "ingenico/direct-sdk-php",
    "type": "library",
    "description": "PHP SDK to communicate with the Ingenico ePayments platform server-to-server API",
    "homepage": "https://support.direct.ingenico.com/documentation/sdk/server/php/",
    "keywords": ["SDK"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ingenico",
            "homepage": "https://support.direct.ingenico.com/documentation/sdk/server/php/"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
        "psr-4": {
            "Ingenico\\Direct\\Sdk\\": ["src/Ingenico/Direct/Sdk", "lib/Ingenico/Direct/Sdk"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ingenico\\Direct\\Sdk\\": ["tests/Ingenico/Direct/Sdk"]
        }
    }
}
